int main(void)
{  
선택정렬
  
  int P=0;
  
  for(int i=0; i<MAX_SIZE; i++)
  {
    for(int j=i+1; j<MAX_SIZE; j++)
    {
      if(A[i]>A[j])
      {
        int temp=A[i];
        A[i]=A[j];
        A[j]=temp;
      }
      
    }
    
  }

  for(int T=0; T<MAX_SIZE; T++)
    printf("%3d", A[T]);

  return 0; 
}
